          "$id": "#/properties/users/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "_id",
            "email",
            "password",
            "name",
            "birth",
            "cellPhone",
            "city",
            "district",
            "nationality",
            "picture",
            "rating",
            "__v"
          ],
          "properties": {
            "_id": {
              "$id": "#/properties/users/items/properties/_id",
              "type": "object",
              "title": "The _id Schema",
              "required": [
                "$oid"
              ],
              "properties": {
                "$oid": {
                  "$id": "#/properties/users/items/properties/_id/properties/$oid",
                  "type": "string",
                  "title": "The $oid Schema",
                  "default": "",
                  "examples": [
                    "5c4f10c572ab1a89624336cb"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            },
            "email": {
              "$id": "#/properties/users/items/properties/email",
              "type": "string",
              "title": "The Email Schema",
